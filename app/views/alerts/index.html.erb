<h1>Alerts</h1>
<div class="container">
<table class="table">
  <thead>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>date</th>
  </tr>
  </thead>
  <tbody>
  <% @alerts.each do |alert| %>
    <tr>
      <td>
        <%=alert.title %>
    </td>
      <td>
        <%=alert.details %>
      </td>
      <td>
        <%=alert.created_at %>
      </td>
  </tr>

  <%end %>
  </tbody>
</table>
</div>